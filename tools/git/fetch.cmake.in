execute_process(
	COMMAND @ENV_GIT_SILENT_COMMAND@ fetch origin
	WORKING_DIRECTORY @GIT_REPO@
	RESULT_VARIABLE GIT_RESULT
	OUTPUT_VARIABLE GIT_OUT
	ERROR_VARIABLE GIT_ERROR
)

if(NOT GIT_RESULT EQUAL 0)
	message(FATAL_ERROR "Git fetch failed (exit ${GIT_RESULT}). See error output above for details.")
endif()